{
  "name": "debug-es",
  "version": "1.2.0",
  "description": "small debugging utility like 'debug', but write in es6",
  "repository": {
    "type": "git",
    "url": "https://github.com/LightouchDev/debug-es"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.mjs",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "cross-env BABEL_ENV=cjs rollup --config",
    "build:esm": "cross-env BABEL_ENV=esm rollup --config",
    "coverage": "jest --verbose --coverage --collectCoverageFrom=src/**/*",
    "test": "jest --verbose"
  },
  "keywords": [
    "debug",
    "logging"
  ],
  "author": "TJ Holowaychuk <tj@vision-media.ca>",
  "contributors": [
    "Nathan Rajlich <nathan@tootallnate.net> (http://n8.io)",
    "Andrew Rhyne <rhyneandrew@gmail.com>",
    "Miau Lightouch <miaulightouch@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "ms": "^2.1.1",
    "supports-color": "^5.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.0",
    "@babel/preset-env": "^7.0.0-rc.0",
    "@babel/runtime": "^7.0.0-rc.1",
    "babel-eslint": "^8.2.6",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.20.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.0-beta.8",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1"
  }
}
