{
  "env": {
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "targets": {
            "esmodules": true
          }
        }]
      ]
    },
    "esm": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "targets": {
            "node": 4
          }
        }]
      ]
    },
    "umd": {
      "presets": ["@babel/preset-env"]
    },
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": true
          }
        }]
      ],
      "plugins": ["add-module-exports"]
    }
  }
}
